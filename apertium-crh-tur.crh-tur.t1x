    <rule comment="REGLA: ve ilâhre ">
      <pattern>
        <pattern-item n="ve"/>
        <pattern-item n="ilâhre"/>
      </pattern>
      <action>
      <call-macro n="f_strip_tags"><with-param pos="1"/></call-macro>
        <let>
          <clip pos="1" side="tl" part="a_non_fin"/><lit-tag v="dub.aor"/>
        </let>
        <let>
          <clip pos="1" side="tl" part="a_cas"/><lit-tag v="p3.sg"/>
        </let>
        <out>
          <chunk name="n" case="caseSecondWord">
            <tags>
              <tag><lit-tag v="SV"/></tag>
            </tags>
            <lu>
              <clip pos="2" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="REGLA: vaux_ger_past ">
