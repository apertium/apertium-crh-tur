      <action>
      <call-macro n="f_strip_tags"><with-param pos="1"/></call-macro>
        <let>
          <clip pos="1" side="tl" part="a_tags"/>
<concat><lit-tag v="v"/><clip pos="1" side="tl" part="a_tra"/><lit-tag v="past"/></concat>
        </let>
        <out>
          <chunk namefrom="v" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SV"/></tag>
              <tag><clip pos="1" side="tl" part="a_fin"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>


    <rule comment="REGLA: er eken ">
      <pattern>
        <pattern-item n="er"/>
        <pattern-item n="eken"/>
      </pattern>
      <action>
      <call-macro n="f_strip_tags"><with-param pos="1"/></call-macro>
        <let>
          <clip pos="1" side="tl" part="a_non_fin"/><lit-tag v="ger_aor"/>
        </let>
        <let>
          <clip pos="1" side="tl" part="whole"/>
          <concat><clip pos="1" side="tl" part="whole"/><lit-tag v="nom"/></concat>
        </let>
        <let>
          <clip pos="2" side="tl" part="whole"/>
          <concat><lit v="+i"/><lit-tag v="cop.gna_ken"/></concat>
        </let>
        <out>
          <chunk name="v" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SV"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
              <clip pos="2" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>




    <rule comment="REGLA: opt">
      <pattern>
        <pattern-item n="opt"/>
      </pattern>
      <action>
      <call-macro n="f_strip_tags"><with-param pos="1"/></call-macro>
<choose>
   <when>
