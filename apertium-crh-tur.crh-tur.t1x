  <section-def-macros>

    <def-macro n="f_strip_tags" npar="1">
          <let>
            <clip pos="1" side="tl" part="a_err"/>
            <lit v=""/>
          </let>
    </def-macro>
    <def-macro n="f_poss_xd1" npar="1">
        <choose>
         <when><test><equal><clip pos="1" side="tl" part="a_poss"/><lit v=""/></equal></test>
        <let><clip pos="1" side="tl" part="a_xd"/><lit-tag v="px3sp"/></let>
        </when>
        <otherwise>
        <let><clip pos="1" side="tl" part="a_xd"/><lit v=""/></let>
        </otherwise></choose>
    </def-macro>

    <def-macro n="f_3sg_if_empty1" npar="1">
      <let><var n="pers-nbr"/><lit v=""/></let>
      <choose>
        <when><test><equal><clip pos="1" side="tl" part="a_pers_nbr"/><lit v=""/></equal></test>
              <let><var n="pers-nbr"/><lit-tag v="p3.sg"/></let></when>
        <otherwise>
              <let><var n="pers-nbr"/><clip pos="1" side="tl" part="a_pers_nbr"/></let>
        </otherwise>
      </choose>
    </def-macro>

    <def-macro n="f_fut2_fut1" npar="1">
 <choose>
   <when>
       <!-- IF: a_fin = fut2 -->
      <test><equal><clip pos="1" side="tl" part="a_fin"/> 
                             <lit-tag v="fut2"/></equal></test>
        <!-- THEN: LET a_fin be fut -->
      <let><clip pos="1" side="tl" part="a_fin"/><lit-tag v="fut"/></let>
    </when>
</choose>
    </def-macro>

